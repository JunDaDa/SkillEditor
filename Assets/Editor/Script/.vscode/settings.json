{
    "workbench.editor.enablePreview": false,
    "editor.renderWhitespace": "all",
    "workbench.colorTheme": "Monokai",
    "files.exclude": {
        "**/.svn": true,
        "**/*.meta": true,
		    ".gitattributes": true,
		    ".gitignore": true
    },
    "files.associations": {
        "**/*.bytes": "lua",
        "*.bytes": "lua"
    },
    "editor.detectIndentation": true,
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
}